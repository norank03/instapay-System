<?php
class User{
 private $ipa;
 private $password;
 private $phone;

 private $FirstName;
 private $LastName;
 private $email;


 public function getfname(){
    return $this->FirstName;
}
public function getlname(){
    return $this->LastName;
}
public function getemail(){
    return $this->email;
}
public function getipa(){
    return $this->ipa;
}

public function getpassword(){
    return $this->password;
}

public function getphone(){
    return $this->phone;
}


public function setipa($pa){
     $this->ipa=$pa;
}

public function setpassword($pass){
    $this->password=$pass;
}
public function setfname($fnam){
    $this->FirstName=$fnam;
}
public function setlname($lnam){
    $this->LastName=$lnam;
}
public function setemail($em){
    $this->email=$em;
}
public function setphone($mob){
    $this->phone=$mob;
}

}
?>
